{
  "name": "nextjs15-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check": "biome check --error-on-warnings --no-errors-on-unmatched && next lint",
    "check:fix": "biome check --error-on-warnings --no-errors-on-unmatched --write",
    "typecheck": "tsc --noEmit",
    "sb": "storybook dev -p 6006 --no-open --no-version-updates --enable-crash-reports false",
    "sb:build": "storybook build"
  },
  "dependencies": {
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@chromatic-com/storybook": "^4.0.0",
    "@storybook/addon-a11y": "^9.0.12",
    "@storybook/addon-docs": "^9.0.12",
    "@storybook/addon-onboarding": "^9.0.12",
    "@storybook/addon-vitest": "^9.0.12",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/experimental-nextjs-vite": "^8.6.14",
    "@storybook/nextjs": "^9.0.12",
    "@tailwindcss/postcss": "^4",
    "@tsconfig/next": "^2.0.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.2",
    "lefthook": "^1.11.14",
    "storybook": "^9.0.12",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
